{
  "name": "Twitch-analyser-job",
  "version": "1.0.0",
  "description": "Job analysing twitch traffic.",
  "main": "index.js",
  "scripts": {
    "start": "node ./index.js",
    "dev": "NODE_ENV=run nodemon -L ./index.js",
    "pretest": "npm run lint",
    "test": "NODE_ENV=test jest --collectCoverage --silent --testPathPattern=/test --maxWorkers=1 --forceExit --passWithNoTests",
    "lint": "eslint .",
    "mock:db": "node ./script/setMockUsers.js"
  },
  "babel": {
    "plugins": [
      "@babel/plugin-proposal-throw-expressions"
    ]
  },
  "jest": {
    "verbose": true,
    "roots": [
      "<rootDir>",
      "<rootDir>/test/"
    ],
    "collectCoverageFrom": [
      "<rootDir>/application/**/*.js/",
      "<rootDir>/domain/**/*.js/",
      "<rootDir>/infrastructure/**/*.js/"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/EsteveSegura/Twitch-analyser-job.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/EsteveSegura/Twitch-analyser-job/issues"
  },
  "homepage": "https://github.com/EsteveSegura/Twitch-analyser-job#readme",
  "dependencies": {
    "awilix": "^6.1.0",
    "axios": "^0.26.1",
    "dotenv": "^16.0.0",
    "mongodb": "3.6.9",
    "node-cron": "^3.0.0",
    "uuid": "^8.3.2",
    "uuid-mongodb": "2.4.4"
  },
  "devDependencies": {
    "@babel/plugin-proposal-throw-expressions": "^7.14.5",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.30.0",
    "eslint-config-google": "^0.14.0",
    "jest": "^26.4.2",
    "nodemon": "^2.0.15"
  }
}
